section.login
  = form_with scope: :session, class:'box', local: true do |f|
    h1 LogIn
    // = render partial: 'shared/error_message', locals: { model: f.object }

    = f.text_field :email, placeholder: 'Email', id: 'session_email'

    = f.password_field :password, placeholder: 'Password', id: 'session_password'

    = f.submit 'LogIn'
